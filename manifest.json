{
    "manifest_version" : 6,
    "name" : "PeopleChain",
    "version" : "0.1",
    "content_scripts" : [
        {
        "matches" : ["<all_urls>"],
        "js":["./views/popup.html"]
}],
"permissions" : ["storage"],
    "browser_action" :{
        "default_popup" : "./views/popup.html",
        "default_title" : "Peoplechain"
    },
    "background" : {
        "scripts":["./javascript/background.js"]}
}